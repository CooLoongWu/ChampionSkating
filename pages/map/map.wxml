<!--pages/map.wxml-->
<view>
<map id="map" wx:if="{{load}}" longitude="{{longitude}}" latitude="{{latitude}}" scale="15" markers="{{markers}}" show-location style="width: 100%; height: 90vh"
bindmarkertap='clickMarker' bindcallouttap='clickCallout'></map>

<button bindtap='chooseLocation'>位置选择</button>
</view>
